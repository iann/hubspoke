<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<html lang="en">
  <head>
    {% block starthead %}{% endblock starthead %}
    <meta charset="{{ resource.meta.charset }}">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title %}{{ resource.meta.title }}{% endblock %} | {{ site_title }}</title>
    <meta name="description" content="{{ resource.meta.description }}">
    <meta name="viewport" content="width=device-width">
    <meta name="author" content="{{ resource.meta.author }}">

    <!--[if lt IE 9]>
      <script src="js/vendor/html5-3.6-respond-1.1.0.min.js"></script>
    <![endif]-->

    {% block basecss %}
    <link href="{{ media_url('css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ media_url('css/bootstrap-responsive.min.css') }}" rel="stylesheet">
    {% endblock basecss %}

    {% block css %}
    <link href="{{ media_url('css/style.css') }}" rel="stylesheet">{% endblock %}



    {% block favicons %}
    {# Place favicon.ico & apple-touch-icon.png in the root of your domain and delete these references #}
    <link rel="shortcut icon" href="{{ media_url('img/favicon.ico') }}">
    <link rel="apple-touch-icon" href="{{ media_url('img/apple-touch-icon.png') }}">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ media_url('img/appleimgim-touch-icon-72x72.png') }}">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ media_url('img/apple-touch-icon-114x114.png') }}">
    {% endblock favicons %}
    
    {% block headjs %}
    {% endblock headjs %}

    {% block endhead %}{% endblock endhead %}
    {% block analytics %}
    {% include "analytics.j2" %}
    {% endblock analytics %}
  </head>

  <body class="{{ resource.meta.id if resource.meta.id else resource.slug }}">

    {% block body %}

    <div class="container" id="top">
      {% block container %}

      <div class="content">
        {% block viz %}
        {% endblock %}
        {# This is where the bulk of the content will go #}
        {% block content %}
        {% endblock %}
      </div>

      <footer>
        {% block footer %}
        <p>Created by <a href="{{ author.url }}">{{ author.name }}</a></p>
        {% endblock footer %}
      </footer>

      {% endblock container %}
    </div> <!-- /container -->

    {% endblock body %}
    
    {% block js %}
    {# Javascript at the bottom for fast page loading #}

    {% block jquery %}

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/lib.min/jquery.js"><\/script>')</script>
    <!-- <script src="{{ media_url('js/lib.min/jquery.js') }}"></script> -->
    {% endblock jquery %}

    {% block bootstrap_js %}
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.1.1/bootstrap.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/lib.min/bootstrap.js"><\/script>')</script>

    <!-- <script src="{{ media_url('js/lib.min/bootstrap.min.js') }}"></script> -->

    {% endblock bootstrap_js %}

    {% block d3_js %}
    <script src="//cdnjs.cloudflare.com/ajax/libs/d3/2.10.0/d3.v2.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/lib/d3.v2.js"><\/script>')</script>

    <script src="{{ media_url('js/viz.js') }}"></script>

    {% endblock d3_js %}

    {% block scripts %}
    {% endblock scripts %}

    <!--[if lt IE 7 ]>
      <script src="{{ media_url('js/libs/dd_belatedpng.js') }}"></script>
      <script>DD_belatedPNG.fix('img, .png_bg'); // Fix any <img> or .png_bg bg-images. Also, please read goo.gl/mZiyb </script>
    <![endif]-->

    {% endblock js %}
  </body>
</html>
